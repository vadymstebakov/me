{
    "name": "me",
    "type": "module",
    "version": "0.0.1",
    "engines": {
        "node": ">=22",
        "yarn": "please-use-npm",
        "pnpm": "please-use-npm"
    },
    "scripts": {
        "dev": "astro dev",
        "build": "astro build",
        "preview": "astro preview",
        "astro": "astro",
        "lint:eslint": "eslint --cache --max-warnings 0 --report-unused-disable-directives \"**/*.{js,ts,mjs,cjs,astro,tsx,jsx}\"",
        "lint:stylelint": "stylelint --cache -q \"**/*.{css,astro}\"",
        "lint:prettier": "prettier --check \"**/*.{js,ts,tsx,jsx,json,astro,html,css,mjs,cjs}\"",
        "lint:prettier:fix": "prettier --write \"**/*.{js,ts,tsx,jsx,json,astro,html,css,mjs,cjs}\"",
        "lint": "astro check && npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier"
    },
    "dependencies": {
        "@astrojs/check": "0.9.5",
        "astro": "5.16.0",
        "typescript": "5.9.3"
    },
    "devDependencies": {
        "@astrojs/ts-plugin": "1.10.5",
        "@typescript-eslint/parser": "8.47.0",
        "autoprefixer": "10.4.22",
        "eslint": "9.39.1",
        "eslint-plugin-astro": "1.5.0",
        "eslint-plugin-jsx-a11y": "6.10.2",
        "postcss-html": "1.8.0",
        "prettier": "3.6.2",
        "prettier-plugin-astro": "0.14.1",
        "stylelint": "16.25.0",
        "stylelint-config-html": "1.1.0",
        "stylelint-config-standard": "39.0.1"
    }
}
